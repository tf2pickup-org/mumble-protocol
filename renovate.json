{
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "automerge": true
    },
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "labels": ["renovate"],
  "semanticCommits": "enabled"
}
